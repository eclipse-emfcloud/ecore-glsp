{
  "private": true,
  "engines": {
    "yarn": "1.0.x || >=1.2.1",
    "node": ">=10.11.0 <13"
  },
  "scripts": {
    "test": "yarn",
    "prepare": "lerna run prepare",
    "rebuild:browser": "theia rebuild:browser",
    "rebuild:electron": "theia rebuild:electron",
    "watch": "lerna run --parallel watch",
    "start": "cd browser-app && yarn start",
    "start:debug": "cd browser-app && yarn start:debug",
    "publish": "yarn && yarn publish:latest",
    "publish:latest": "lerna publish",
    "publish:next": "lerna publish --exact --canary=next --npm-tag=next --yes",
    "testcafe:start": "testcafe chrome:headless",
    "testcafe:start:visual": "testcafe chrome",
    "e2etest": "npm-run-all --parallel --race --aggregate-output start testcafe:start",
    "e2etest:visual": "npm-run-all --parallel --race --aggregate-output start testcafe:start:visual"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "babel-loader": "8.0.6",
    "eslint": "^6.8.0",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-react": "^7.20.3",
    "lerna": "^2.11.0",
    "npm-run-all": "^4.1.5",
    "testcafe": "^1.11.0"
  },
  "resolutions": {
    "**/@theia/application-manager": "^1.4.0",
    "**/@theia/application-package": "^1.4.0",
    "**/@theia/core": "^1.4.0",
    "**/@theia/editor": "^1.4.0",
    "**/@theia/filesystem": "^1.4.0",
    "**/@theia/languages": "^1.4.0",
    "**/@theia/markers": "^1.4.0",
    "**/@theia/messages": "^1.4.0",
    "**/@theia/monaco": "^1.4.0",
    "**/@theia/navigator": "^1.4.0",
    "**/@theia/preferences": "^1.4.0",
    "**/@theia/process": "^1.4.0",
    "**/@theia/terminal": "^1.4.0",
    "**/@theia/workspace": "^1.4.0",
    "**/sprotty": "0.9.0",
    "**/sprotty-theia": "0.9.0",
    "**/@eclipse-glsp/client": "0.8.0-next.5c0863ff",
    "**/@eclipse-glsp/theia-integration": "0.8.0-next.cef5529d",
    "**/@eclipse-emfcloud/modelserver-theia": "0.8.0-next.5bf1df0",
    "**/@eclipse-emfcloud/theia-tree-editor": "0.7.0-next.3da9d6a",
    "**/react": "^16.12.0"
  },
  "workspaces": [
    "browser-app",
    "configs",
    "packages/*/"
  ],
  "dependencies": {
    "yarn": "^1.22.4"
  }
}
