{
  "private": true,
  "engines": {
    "yarn": "1.0.x || >=1.2.1",
    "node": ">=12.14.1 <13"
  },
  "scripts": {
    "test": "yarn",
    "prepare": "lerna run prepare",
    "rebuild:browser": "theia rebuild:browser",
    "rebuild:electron": "theia rebuild:electron",
    "watch": "lerna run --parallel watch",
    "start": "cd browser-app && yarn start",
    "start:debug": "cd browser-app && yarn start:debug",
    "publish": "yarn && yarn publish:latest",
    "publish:latest": "lerna publish",
    "publish:next": "lerna publish --exact --canary=next --npm-tag=next --yes",
    "testcafe:start": "testcafe chrome:headless",
    "testcafe:start:visual": "testcafe chrome",
    "e2etest": "npm-run-all --parallel --race --aggregate-output start testcafe:start",
    "e2etest:visual": "npm-run-all --parallel --race --aggregate-output start testcafe:start:visual"
  },
  "dependencies": {
    "yarn": "^1.22.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "babel-loader": "8.0.6",
    "eslint": "^6.8.0",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-react": "^7.20.3",
    "lerna": "^2.11.0",
    "npm-run-all": "^4.1.5",
    "testcafe": "^1.11.0",
    "vscode-languageclient": "^6.1.3"
  },
  "resolutions": {
    "**/sprotty": "0.10.0-next.0e06051",
    "**/sprotty-theia": "0.10.0-next.a5ea006"
  },
  "workspaces": [
    "browser-app",
    "configs",
    "packages/*/"
  ]
}
